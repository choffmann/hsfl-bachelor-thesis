{
  "name": "@benchmarks/impl",
  "version": "1.0.0",
  "description": "Collection of Benchmarks",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "main:src": "src/index.ts",
  "repository": "https://github.com/choffmann/hsfl-bachelor-thesis",
  "author": "Cedrik Hoffmann",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn build:wasm && tsc && yarn copyfiles",
    "build:wasm": "yarn build:wasm:matrix && yarn build:wasm:mandelbrot",
    "build:wasm:matrix": "cd src/targets/wasm/matrix && wasm-pack build --profiling --no-pack --target web --out-dir ../dist/matrix",
    "build:wasm:mandelbrot": "cd src/targets/wasm/mandelbrot && wasm-pack build --profiling --no-pack --target web --out-dir ../dist/mandelbrot",
    "copyfiles": " yarn copyfiles:matrix && yarn copyfiles:mandelbrot",
    "copyfiles:matrix": "copyfiles -u 1 src/targets/wasm/dist/**/*_bg.wasm* dist/",
    "copyfiles:mandelbrot": "copyfiles -u 1 src/targets/mandelbrot/dist/**/*_bg.wasm* dist/",
    "clean": "rm -rf dist/"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "typescript": "^5.3.3"
  }
}
